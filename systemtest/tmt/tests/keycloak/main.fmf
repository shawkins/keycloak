test:
  ./test.sh
duration: 2h
environment:
  DOCKER_ORG: "keycloak"
  DOCKER_TAG: "test"
  TEST_LOG_DIR: "systemtest/target/logs"
  TESTS: ""
  TEST_GROUPS: ""
  EXCLUDED_TEST_GROUPS: ""
  CLUSTER_OPERATOR_INSTALL_TYPE: bundle
  PARALLEL_TEST_COUNT: 2
  RERUN_FAILED_TEST_COUNT: 2

/smoke:
  summary: Run smoke keycloak test suite
  tags: [smoke]
  duration: 40m
  tier: 1
  environment+:
    TEST_PROFILE: smoke
